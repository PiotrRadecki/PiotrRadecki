<Window x:Class="PiotrRadecki.zad3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PiotrRadecki"
        mc:Ignorable="d"
        Title="zad3" Height="220" Width="200">
    <Grid>
        <DockPanel LastChildFill = "True">
            <Button Content = "Bottom" DockPanel.Dock = "Bottom"/>
            <Button Content = "Left" DockPanel.Dock = "Left"/>
            <Button Content = "Right" DockPanel.Dock = "Right"/>
            <Button Content = "Top" DockPanel.Dock = "Top" Background="red" Height="50"/>
            <Button Content = "Top2" DockPanel.Dock = "Top" Background="green" Height="50" Width="50"/>
            <Button Content = "Right2" DockPanel.Dock = "Right" Height="50"/>
            <Button Content = "Right3" DockPanel.Dock = "Right" Height="50"/>
            <Button Content = "Top3" DockPanel.Dock = "Top" Background="blue" Height="25"/>
            <Button Content = "Fill" DockPanel.Dock = "Top" Height="40" />
        </DockPanel>
    </Grid>
</Window>
